  background url("ossweb-img/bg-comp.png") center no-repeat
.part-item-tit
  margin: 18px 0 18px;
  font-size 24px
  color #a28cd5
.part-item-btn
  display block
  position absolute
  top 305px
  left 68px
  width 175px
  height 44px
  font-size 18px
  text-align center
  color color4
  line-height 44px
  cursor pointer
  background url("ossweb-img/bg-tit-04.png") center center no-repeat
.part-item-2
  margin-left 35px
  margin-right 35px
.get-way
  display none



.panel-3
  height 666px;
  background url("ossweb-img/bg_03.jpg") center center no-repeat
  .content
    position relative

.swiper1-arrow-left,.swiper1-arrow-right
  display block
  position absolute
  top 392px
  width 34px
  height 51px
.swiper1-arrow-left
  left 0
  background url("ossweb-img/sprit.png")
  background-position 0 0
.swiper1-arrow-right
  right 0
  background url("ossweb-img/sprit.png")
  background-position -51px 0


.panel-4
  height 701px
  background url("ossweb-img/bg_04.jpg") center center no-repeat
  .tit-1
    margin-top 64px
    margin-bottom 34px
  .content
    padding 0  41px
    position relative
.swiper2-arrow-left,.swiper2-arrow-right
  display block
  position absolute
  top 400px
  width 26px
  height 35px
  z-index 10
.swiper2-arrow-left
  left 0
  background url("ossweb-img/sprit.png")
  background-position 0 -60px

.swiper2-arrow-right
  right 0
  background url("ossweb-img/sprit.png")
  background-position -54px -60px


