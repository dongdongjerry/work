body
  min-width : 1200px
.content
  width :1000px
  margin : 0 auto
  overflow hidden
.tit-1,
.tit-2,
.tit-3
  margin 0 auto
  margin-top 42px
  padding-right 60px
  text-align center
  line-height 88px
  font-size 36px
  color color1
.tit-1
  width 501px
  height 94px
  background url("ossweb-img/bg-tit-01.png") center center no-repeat
.tit-2
  width 458px
  height 101px
  background url("ossweb-img/bg-tit-02.png") center center no-repeat
.tit-3
  width 486px
  height 101px
  background url("ossweb-img/bg-tit-03.png") center center no-repeat
.panel
  width 100%
.slogan
  display none
.clearFloat
  overflow hidden

/*----bg1------------*/
.panel-1
  height 893px
  background url("ossweb-img/bg1.jpg") center center no-repeat
.logo
  float: right;
  margin-top: 23px;
  margin-right: 200px;
.content-1
  height 893px
  background url("ossweb-img/bg-content.jpg") center no-repeat
/*分割图片*/
