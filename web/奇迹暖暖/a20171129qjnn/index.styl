/*---------bg2-----------*/
.panel-2
  height 615px
  background url("ossweb-img/bg_02.jpg") center center no-repeat
  text-align center
.small-tit
    font-size 14px
    color color2
    padding-right 60px
.part-item
  float left
  position relative
  margin-top 21px
  width 310px
  height 428px
