/*------侧边提示-------*/
.slide-tip
  width 113px
  height 266px
  position fixed
  right: 5%;
  top: 30%;
  background url("ossweb-img/bg-sidebar.png") top center no-repeat
  text-align center
  display none
  z-index 15
  h4
    font-size 14px
    color #fff
    margin-top 108px
    margin-bottom 18px
  p
    font-size 14px
    color color9
    width 101px
    margin-left 7px
    text-align left
    /*text-align left*/
    margin-bottom 11px
  a
    display block
    width 33px
    height 33px
    position absolute
    left 43px
    bottom 0
    /*margin-top 11px*/
    background url("ossweb-img/sprit.png")
    background-position -52px -163px

/*---------配件弹出层--------*/


.popup-comp
  width 100%
  height 3426px
  top 0
  z-index 15
  position absolute
  display none
.popup-wrap
  width 100%
  height 100%
  position absolute
  top 0
  background #1d1a30
  opacity .6
  filter:unquote('alpha(opacity=60)')
.part-detail
  width 433px
  height 522px
  margin 0 auto
  margin-top 970px
  padding-top 1px
  text-align center
  position relative
  z-index 16
  display none
  background url("ossweb-img/bg-part.png") center no-repeat
  img
    position absolute
    top 9px
    left 0
  h4
    position absolute
    top 265px
    left 118px
    width 198px
    height 47px
    margin 0 auto
    background url("ossweb-img/bg-tit-06.png") center no-repeat
    font-size 18px
    color #88798e
    line-height 47px
  h5
    font-size 16px
    color #8d7498
    margin-top 366px
  p
    font-size 14px
    color #978ea8



.player-opus
  width 473px
  height 707px
  margin 0 auto
  margin-top 2180px
  padding-top 1px
  background url("ossweb-img/bg-player-opus.png") center no-repeat
  position relative
  z-index 16
  text-align center
  display none
  img
    width 399px
    height 502px
    margin-top 25px
  h4
    width 175px
    height 44px
    padding 0 10px
    position absolute
    left 139px
    top 505px
    background url("ossweb-img/bg-tit-07.png") center no-repeat
    font-size 18px
    color #fff
    line-height 44px
    text-overflow:ellipsis;
    overflow:hidden;
    white-space:nowrap;
  p
    margin-top 35px
    font-size 16px
    color #8d7498
  span
    display block
    width 316px
    margin 0 auto
    font-size 14px
    color #978ea8



.btn-close
  width 41px
  height 41px
  background url("ossweb-img/btn-close.png") center no-repeat
  position absolute
  top 0
  right -53px
.show,.popup-comp>.show
  display block